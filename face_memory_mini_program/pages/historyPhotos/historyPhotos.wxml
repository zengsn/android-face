<!--pages/historyPhotos/historyPhotos.wxml-->
<view class="page">
  <!-- <view class="page__hd">
        <view class="page__title">Panel</view>
        <view class="page__desc">面板</view>
    </view> -->
  <view class="page__bd">
    <view class="weui-panel weui-panel_access">
      <view class="weui-panel__hd">历史照片</view>
      <view class="weui-panel__bd">
        <view wx:for="{{abbr_url}}" wx:key="unique">

          <view class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image class="weui-media-box__thumb" src="{{item}}" data-index="{{index}}" bindtap='previewImage' />
            </view>
            <navigator url="../detail/detail?id={{faceinfo[index].id}}&faceValue={{faceinfo[index].faceValue}}&age={{faceinfo[index].age}}">
              <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                <view class="weui-media-box__title">{{faceinfo[index].createtime}}</view>
                <view class="weui-media-box__desc">
                  <text decode="{{true}}" space="{{true}}">颜值：{{faceinfo[index].faceValue}}\n 年龄：{{faceinfo[index].age}} &nbsp;&nbsp;&nbsp;</text>
                  <!-- 健康值：{{faceinfo[index].healthValue}} -->
                </view>
              </view>
            </navigator>
          </view>
        </view>
      </view>

    </view>
  </view>
</view>